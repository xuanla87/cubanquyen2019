@model IEnumerable<CucbanquyenModel.Models.Menu>
@if (Model != null)
{
    <div class="box-5 thutucdk">
        <div class="box-content">
            <div class="box-header">
                <span class="title text-center text-capitalize">
                    @Html.Raw(ViewBag.Name)
                </span>
            </div>
            <div class="box-body">
                <ul class="@ViewBag.stclass">
                    @foreach (var item in Model)
                    {
                        <li>
                            <a href="@item.menuUrl">@item.menuName</a>
                        </li>
                    }
                </ul>
            </div>
        </div>
    </div>
  
}

